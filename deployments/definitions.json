{
  "users": [
    {
      "name": "uptimeping",
      "password": "uptimeping123",
      "tags": "administrator",
      "hashing_algorithm": "rabbit_password_hashing_sha256"
    }
  ],
  "vhosts": [
    {
      "name": "/",
      "description": "Default virtual host"
    }
  ],
  "permissions": [
    {
      "user": "uptimeping",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "uptimeping-cluster"
    }
  ],
  "policies": [],
  "queues": [
    {
      "name": "uptimeping.tasks",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 10000
      }
    },
    {
      "name": "uptimeping.notifications",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 5000
      }
    },
    {
      "name": "uptimeping.metrics",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 1000
      }
    }
  ],
  "exchanges": [
    {
      "name": "uptimeping.direct",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "uptimeping.topic",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "uptimeping.fanout",
      "vhost": "/",
      "type": "fanout",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "uptimeping.direct",
      "vhost": "/",
      "destination": "uptimeping.tasks",
      "destination_type": "queue",
      "routing_key": "uptimeping.tasks",
      "arguments": {}
    },
    {
      "source": "uptimeping.direct",
      "vhost": "/",
      "destination": "uptimeping.notifications",
      "destination_type": "queue",
      "routing_key": "uptimeping.notifications",
      "arguments": {}
    },
    {
      "source": "uptimeping.direct",
      "vhost": "/",
      "destination": "uptimeping.metrics",
      "destination_type": "queue",
      "routing_key": "uptimeping.metrics",
      "arguments": {}
    },
    {
      "source": "uptimeping.topic",
      "vhost": "/",
      "destination": "uptimeping.tasks",
      "destination_type": "queue",
      "routing_key": "task.*",
      "arguments": {}
    },
    {
      "source": "uptimeping.topic",
      "vhost": "/",
      "destination": "uptimeping.notifications",
      "destination_type": "queue",
      "routing_key": "notification.*",
      "arguments": {}
    },
    {
      "source": "uptimeping.fanout",
      "vhost": "/",
      "destination": "uptimeping.tasks",
      "destination_type": "queue",
      "routing_key": "",
      "arguments": {}
    }
  ]
}
